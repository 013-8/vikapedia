{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue",
        "src/clients/**/*",
        "src/clients/**/*.ts",
        "src/models/**/*",
        "src/models/**/*.ts",
        "src/models/si/**/*",
        "src/models/si/**/*.ts"
    ],
    "exclude": ["src/**/__tests__/*"],
    "compilerOptions": {
        "composite": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
        }
    }
}
